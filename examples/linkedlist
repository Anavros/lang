
namespace ll {
    Link = Type[T] {
        value = (..):T;
        next = (..):Link;
    }

    LinkedList = Type[T] {
        head = (..):Link[T];
        at_index = Func(list:LinkedList, i:Num)
            : Link { }
        append = Func(list:LinkedList, link:Link)
            { }
    }
}

import ll

list = (..):ll.LinkedList;
ll.at_index(list, 10);
list.at_index(10);
list@10;
