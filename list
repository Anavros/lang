
concat = Func(a:Sequence, b:Sequence): Sequence {
}

concat(a, b)
a.concat(b).concat(c)

concat = Func(seqs:Glob[Sequence]) : Sequence {
    result = (..):Sequence[
        len=sum(s.length() for s in seqs);
    ];
    i = 0;
    for s in seqs {
        for item in s {
            result@i = item;
            i += 1;
        }
    }
    return result;
}

concat(a, b, c, d, e);
