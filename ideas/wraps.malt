
###
Wraps or decorators take functions and execute them with other statements.
###

log = (f:Function) {
    print("Starting execution of {f.name}().");
    r = f();
    print("Finished execution. Received return value {r}.");
}

increment = (n:Num) : Num {
    return n+1;
}
increment = log(increment(?));

sys.entry = {
    increment(5);
}
