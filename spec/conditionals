
# Standard If Trees
if x {
    f();
} elif y {
    g();
} else {
    h();
}
if x { f(); } elif y { g(); } else { h(); }

# Inline Conditional Statements
f() if x;

# Matches
match {
    false { f(); }
    true  { g(); }
}

# Matches with Partial Application
match has(?, iter) {
    x { x.iter(); }
    y { y.iter(); }
}
